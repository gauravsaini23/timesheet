<html>
  <head>
    <title>
      Timesheet
    </title>  
      <%= stylesheet_link_tag "timesheet"%>
      <%= javascript_include_tag :defaults %>
  </head>
  <body id="timesheet">

    <div id="banner_img" >
      <%= link_to image_tag("logo.png", :border => 0), {:action => "home", :controller => "users"} %>
    </div>
    <div id="main">
      <% if flash[:msg]%>
        <div id = "msg_box">
      <%= flash[:msg]%><br/>
        </div>
      <%end%>
      <%= yield :layout %>
    </div> 
      
        
  </body  >
</html>
